{
  "buildCommand": "yarn run build",
  "outputDirectory": "dist",
  "installCommand": "yarn install",
  "framework": "vite",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
